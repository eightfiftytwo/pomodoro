using Gtk 4.0;
using Adw 1;

template $Gjs_Shortcuts : ShortcutsWindow {
  modal: true;

  ShortcutsSection {
    section-name: "shortcuts";
    max-height: 10;
    ShortcutsGroup {
      title: C_("shortcut window", "General");
      ShortcutsShortcut {
        accelerator: "<Control>question";
        title: _("Keyboard Shortcuts");
      }
      ShortcutsShortcut {
        accelerator: "<Control>Q";
        title: _("Quit");
      }
    }
  }
}
