<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="pomodoro">
  <schema id="@app_id@" path="/io/gitlab/idevecore/Pomodoro/" gettext-domain="@app_id@">
    <key type="b" name="run-in-background">
      <default>true</default>
    </key>
    <key type="s" name="theme">
      <default>"default"</default>
    </key>
    <key type="s" name="sort-by">
      <default>"name"</default>
    </key>
    <key type="s" name="order-by">
      <default>"ascending"</default>
    </key>
    <key type="i" name="work-time">
      <default>1500</default>
    </key>
    <key type="i" name="break-time">
      <default>300</default>
    </key>
    <key type="i" name="break-time-after-4-pomodoris">
      <default>1800</default>
    </key>
  </schema>
</schemalist>

